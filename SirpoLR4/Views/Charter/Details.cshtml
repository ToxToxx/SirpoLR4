@model SirpoLR4.Models.Charter

<h2>Детали чартера</h2>

<div class="mb-4">
    <h4>Маршрут: @Model.CititesPath</h4>
    <p>Цена: @Model.Price</p>
    <p>Дата чартера: @Model.CharterDateTime</p>
    <p>Создано: @Model.CreatedAt</p>
    <p>Идентификатор: @Model.Id</p>
</div>

<div>
    <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Редактировать</a>
    <a asp-action="Index" class="btn btn-secondary">Вернуться к списку</a>
</div>
